Algoritmo "PRIMEIRO"

Var

   a : real
   
Inicio

      a <- 2.34;
      
      escreval("*", a:8:2, "*")
      escreval
      
      escreval("Olá Mundo!")
      escreval
      escreval("Bom dia!")
      escreval
      escreval("Boa tarde!")
      escreval
      escreva("Boa noite!")
      escreval
      
Fimalgoritmo